                    <option value="jabalpur" data-testing-id="jabalpur">Jabalpur</option>
                    <option value="panvel" data-testing-id="panvel">Panvel</option>
                    <option value="kochi" data-testing-id="kochi">Kochi</option>
                    <option value="delhi" data-testing-id="delhi">Delhi</option>
