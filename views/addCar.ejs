                    <option value="bangalore" data-testing-id="bangalore">Bangalore</option>
                    <option value="nagpur" data-testing-id="nagpur">Nagpur</option>
                    <option value="bhopal" data-testing-id="bhopal">Bhopal</option>
                    <option value="indore" data-testing-id="indore">Indore</option>
